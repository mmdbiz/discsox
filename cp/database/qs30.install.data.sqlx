INSERT INTO `help` VALUES (31, 'encryption', '', 'security_phrase', 'The security phrase below is used for MySQL to store the credit card information in the database using AES encryption. The values above have no effect on this and it should be set at all times.<br /><br /><b>NOTE:</b> Changing this value is not recommended once it has been set and orders have been saved because you will no longer be able to read the card values of old orders.');
