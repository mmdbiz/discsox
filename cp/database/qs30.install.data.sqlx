INSERT INTO `help` VALUES (22, 'encryption', 'This section allows you to encrypt your order emails using PGP/GNUPG or our default encryption system. <u>We strongly recommend that you to use PGP/GNUPG when it\'s available on your web server.</u><br /><br />QuikStore 2.12 users: Please note that this is NOT the same encryption used in the 2.12 version of QuikStore and requires a different encryption tool.', '', '');
